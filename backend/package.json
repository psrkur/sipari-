{
  "name": "yemek5-backend",
  "version": "1.0.0",
  "description": "Fast Food Sales Backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npx prisma generate && npx prisma db push --accept-data-loss",
    "postinstall": "npx prisma generate",
    "db:migrate": "npx prisma db push --accept-data-loss",
    "db:seed": "node -e \"require('./server.js').seedData()\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "bcrypt": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "@prisma/client": "^5.0.0",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "prisma": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "fast-food",
    "api",
    "express",
    "prisma",
    "postgresql"
  ],
  "author": "Your Name",
  "license": "MIT"
}
